  },
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@reach/dialog": "^0.16.2",
    "@tailwindcss/forms": "^0.3.4",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "debounce-fn": "^4.0.0",
    "faker": "^5.5.3",
    "history": "^5.0.0",
    "match-sorter": "^6.3.0",
    "msw": "^0.29.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-icons": "^4.2.0",
    "react-query": "2.1.1",
    "react-query-devtools": "2.3.3",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "stop-runaway-react-effects": "^2.0.0",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test start:cli http://localhost:3000/list cy:open",
    "test:e2e:run": "start-server-and-test serve http://localhost:8811/list cy:run",
    "serve": "serve --no-clipboard --single --listen 8811 build",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "autoprefixer": "^9.8.8",
    "cross-env": "^7.0.3",
    "cypress": "^9.0.0",
    "cypress-hmr-restarter": "^2.0.2",
    "eslint-plugin-cypress": "^2.11.3",
    "husky": "4.3.8",
    "inquirer": "^8.1.0",
    "is-ci": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest-watch-typeahead": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.39",
    "prettier": "^2.3.1",
    "react-test-renderer": "^17.0.2",
    "resolve": "^1.20.0",
    "serve": "^12.0.0",
    "start-server-and-test": "^1.12.5",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
